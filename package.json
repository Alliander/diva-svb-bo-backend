{
  "name": "kpbackend",
  "version": "1.0.0",
  "description": "Ketenpartner backend",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon -e js,html src/server.js",
    "prod": "node src/server.js",
    "lint": "./node_modules/.bin/eslint -c .eslintrc src",
    "test": "mocha 'src/**/*.e2e.js'",
    "test:watch": "mocha --watch 'src/**/*.e2e.js'",
    "test:unit": "mocha 'src/**/*.spec.js'"
  },
  "private": true,
  "keywords": [
    "irma",
    "attributes",
    "privacy",
    "identity",
    "management"
  ],
  "contributors": [
    "Koen van Ingen",
    "Timen Olthof",
    "Sietse Ringers",
    "Alliander N.V. (https://alliander.com/)"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "axios": "0.18.1",
    "express": "4.16.1",
    "express-http-proxy": "1.1.0",
    "promise-readable": "3.1.3",
    "qr-image": "3.2.0",
    "ssh2-sftp-client": "2.0.1",
    "stream-to-promise": "2.2.0",
    "superagent": "3.8.0",
    "underscore": "1.8.3",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "eslint": "4.8.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jasmine": "2.9.1",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-react": "7.4.0",
    "mocha": "4.0.1",
    "supertest": "3.0.0",
    "supertest-as-promised": "4.0.2"
  }
}
